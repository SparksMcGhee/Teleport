# Teleport Deployment Configuration
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Teleport Configuration
Teleport_Public_Name=teleport.example.com
Teleport_Public_Proxy_Hostname=teleport-public.example.com
Teleport_Version=15.0.0

# Public Node Configuration (Digital Ocean Droplet)
Public_Node_IP=1.2.3.4
Public_Node_User=root
Public_Node_SSH_Key=/path/to/public_node_ssh_key
Public_Node_SSH_Cert=/path/to/public_node_ssh_cert

# Private Node Configuration (Home Network VM)
Private_Node_IP=192.168.1.100
Private_Node_User=root
Private_Node_SSH_Key=/path/to/private_node_ssh_key
Private_Node_SSH_Cert=/path/to/private_node_ssh_cert

# Teleport Join Token (generated during public node deployment)
Teleport_Join_Token=

# ACME Configuration (for Let's Encrypt certificates)
ACME_Email=admin@example.com

# Application Access Configuration
Teleport_App_Haos_URL=haos:8123

